
public class MyJavaInherutence {

	public static void main(String[] args) {
	calculator(45,5);
	String();
	
	}
	public static void calculator(int Peter,int Ron){
		int Rafiq=Peter/Ron;
		System.out.println(Rafiq);
		
		
		
	}

	public static void String() {
		
		int fish=27;
		int meat=47;
		int goat=fish-meat;
		System.out.println(goat);
				
		
	}

		
	}

}
